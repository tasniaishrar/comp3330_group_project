{
  "expo": {
    "name": "Expense Tracker",
    "slug": "Expense_Tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/and.png",
    "splash": {
      "image": "./assets/and.png",
      "resizeMode": "contain",
      "backgroundColor": "#97B973"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/and.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    },
    "plugins": [
      "react-native-vision-camera"
    ]

  }
}
